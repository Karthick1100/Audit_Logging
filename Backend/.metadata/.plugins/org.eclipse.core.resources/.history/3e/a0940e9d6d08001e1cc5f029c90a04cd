package com.audit.audit.model;

import java.util.Arrays;

public class FormChange {
	
	public FormChange() {};
	public FormChange(int changeId, int id, String[] changedFields, String changeDescription, String changedAt,
			String changedBy) {
		super();
		this.changeId = changeId;
		this.id = id;
		this.changedFields = changedFields;
		this.changeDescription = changeDescription;
		this.changedAt = changedAt;
		this.changedBy = changedBy;
	}
	int changeId;
	int id;
	String[] changedFields;
	String changeDescription;
	String changedAt;
	String changedBy;
	@Override
	public String toString() {
		return "FormChange [changeId=" + changeId + ", id=" + id + ", changedFields=" + Arrays.toString(changedFields)
				+ ", changeDescription=" + changeDescription + ", changedAt=" + changedAt + ", changedBy=" + changedBy
				+ "]";
	}
	public int getChangeId() {
		return changeId;
	}
	public void setChangeId(int changeId) {
		this.changeId = changeId;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String[] getChangedFields() {
		return changedFields;
	}
	public void setChangedFields(String[] changedFields) {
		this.changedFields = changedFields;
	}
	public String getChangeDescription() {
		return changeDescription;
	}
	public void setChangeDescription(String changeDescription) {
		this.changeDescription = changeDescription;
	}
	public String getChangedAt() {
		return changedAt;
	}
	public void setChangedAt(String changedAt) {
		this.changedAt = changedAt;
	}
	public String getChangedBy() {
		return changedBy;
	}
	public void setChangedBy(String changedBy) {
		this.changedBy = changedBy;
	}
}
